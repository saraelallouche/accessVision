{
    "version": 2,
    "functions": {
      "api/backend/**/*.py": {
        "runtime": "python3.9",
        "includeFiles": "api/backend/**/*"
      }
    },
    "builds": [
      {
        "src": "accessVisionFront",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/accessVisionFront/dist/$1",
        "continue": true
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/backend/$1",
        "continue": true
      }
    ]
  }
  